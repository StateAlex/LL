<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="Student" table="STUDENT">
<id name="id" column="ID" type="integer">
	<generator class="assigned"></generator>
</id>
<property name="nume" column="NUME" type="string"></property>
<property name="prenume" column="PRENUME" type="string"></property>
<property name="an" column="AN" type="integer"></property>
<property name="restante" column="RESTANTE" type="integer"></property>
<property name="nrMatricol" column="NRMATRICOL" type="string"></property>

</class>
</hibernate-mapping>